<div class="product">
  <div class="container">
    <div class="row">
      <% data.products.each do |product|%>
      <div class="col-xs-3">
        <div class="member">
          <h2><%= product.name %></h2>
          <%= image_tag product.image, class: "img-circle"%>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>

